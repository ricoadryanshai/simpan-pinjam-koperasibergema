{
  "devDependencies": {
    "npm-run-all": "^4.1.5"
  },
  "scripts": {
    "dev": "npm-run-all -p dev:*",
    "build": "npm-run-all --parallel client:build server:build",
    "dev:client": "cd client && vite --base /kpgansel/",
    "client:build": "cd client && npm run build-all",
    "dev:server": "cd server && nodemon server.js",
    "server:build": "cd server && npm run build-all"
  },
  "dependencies": {
    "react-to-print": "^2.14.15"
  }
}
