{
  "devDependencies": {
    "npm-run-all": "^4.1.5"
  },
  "scripts": {
    "dev": "npm-run-all -p dev:*",
    "build": "npm-run-all --parallel client:build server:build",
    "dev:client": "cd client && vite --base /kpgansel/",
    "client:build": "cd client && npm run build-all",
    "dev:server": "cd server && nodemon server.js",
    "server:build": "cd server && npm run build-all"
  },
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@fortawesome/free-solid-svg-icons": "^6.4.2",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@headlessui/react": "^1.7.17",
    "@heroicons/react": "^2.0.18",
    "@mui/icons-material": "^5.14.18",
    "@mui/material": "^5.14.18",
    "@mui/x-date-pickers": "^6.18.2",
    "axios": "^1.6.2",
    "body-parser": "^1.20.2",
    "bootstrap": "^5.3.2",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "material-react-table": "^2.0.4",
    "multer": "^1.4.5-lts.1",
    "mysql": "^2.18.1",
    "react": "^18.2.0",
    "react-bootstrap": "^2.9.1",
    "react-dom": "^18.2.0",
    "react-icons": "^4.12.0",
    "react-query": "^3.39.3",
    "react-router-dom": "^6.20.0"
  }
}
